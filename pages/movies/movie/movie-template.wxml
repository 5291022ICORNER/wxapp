<import src="../stars/stars-template.wxml" />
<template name="movie">
  <view class="movie-container">
    <view class="movie-figure">
      <image class="movie-img" src="{{coverimgUrl}}"></image>
      <text class="movie-title">{{title}}</text>
    </view>
    <template is="stars" data="{{score}}"></template>
  </view>
</template>